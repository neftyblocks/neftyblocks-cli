    "assets:mint": {
      "id": "assets:mint",
      "description": "Mints assets in batches using a spreadsheet.",
      "strict": true,
      "pluginName": "@nefty/cli",
      "pluginAlias": "@nefty/cli",
      "pluginType": "core",
      "aliases": [],
      "examples": [
        "<%= config.bin %> <%= command.id %> test.xls -c alpacaworlds"
      ],
      "flags": {
        "batchSize": {
          "name": "batchSize",
          "type": "option",
          "char": "b",
          "description": "Transactions batch size",
          "required": false,
          "multiple": false,
          "default": 100
        },
        "collectionName": {
          "name": "collectionName",
          "type": "option",
          "char": "c",
          "description": "Collection name",
          "required": true,
          "multiple": false
        },
        "ignoreSupply": {
          "name": "ignoreSupply",
          "type": "boolean",
          "char": "i",
          "description": "Ignore supply errors",
          "allowNo": false
        }
      },
      "args": {
        "input": {
          "name": "input",
          "description": "Excel file with the templates and amounts",
          "required": true
        }
      }
    },
