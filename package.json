{
  "name": "neftyblocks-cli",
  "version": "0.0.3",
  "description": "The NeftyBlocks cli will help you manage your collection with commands to create templates, mint assets, and more.",
  "author": "Neftyblocks",
  "bin": {
    "nefty": "./bin/run"
  },
  "homepage": "https://github.com/neftyblocks/neftyblocks-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "neftyblocks/nefty-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "atomicassets": "^1.5.1",
    "elliptic": "^6.5.4",
    "eosjs": "^22.1.0",
    "node-fetch": "2.6.7",
    "read-excel-file": "^5.6.1",
    "write-excel-file": "^1.4.27"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.25",
    "@types/chai": "^4",
    "@types/elliptic": "^6.4.14",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.38",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "chai": "^4",
    "eslint": "^8.49.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^3.0.3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6"
  },
  "oclif": {
    "bin": "nefty",
    "dirname": "nefty",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Commands to configure the cli."
      },
      "templates": {
        "description": "Commands to manage a collection's templates."
      },
      "assets": {
        "description": "Commands to manage a collection's assets."
      },
      "generate": {
        "description": "Commands to generate a files for batch commands."
      }
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "pretest": "yarn build && tsc -p test --noEmit",
    "test": "mocha --forbid-only -r ts-node/register \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "bugs": "https://github.com/neftyblocks/neftyblocks-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "lint-staged": {
    "*.ts": [
      "eslint",
      "prettier --check"
    ]
  }
}
